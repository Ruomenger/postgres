{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "pg_ctl start",
            "program": "${env:HOME}/local/tools/psql/bin/pg_ctl",
            "args": [
                "-D",
                "${env:HOME}/local/tools/psql/data",
                "-l",
                "${workspaceFolder}/.vscode/logpg.log",
                "start"
            ],
            "cwd": "${env:HOME}"
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "pg_ctl status",
            "program": "${env:HOME}/local/tools/psql/bin/pg_ctl",
            "args": [
                "-D",
                "${env:HOME}/local/tools/psql/data",
                "-l",
                "${workspaceFolder}/.vscode/logpg.log",
                "status"
            ],
            "cwd": "${env:HOME}"
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "initdb",
            "program": "/usr/local/pgsql/bin/initdb",
            "args": [
                "-D",
                "/usr/local/pgsql/data"
            ],
            "cwd": "${env:HOME}"
        }
    ]
}
